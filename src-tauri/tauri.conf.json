{
  "productName": "Sage",
  "version": "0.11.1",
  "identifier": "com.rigidnetwork.sage",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "pnpm run dev",
    "beforeBuildCommand": "pnpm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Sage",
        "width": 1150,
        "height": 750,
        "resizable": true,
        "maximized": false,
        "visible": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "iOS": {
      "developmentTeam": "NQJQRYZZG3"
    }
  },
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json"
}
